  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/playerony/lifestyle-blog/issues"
  },
  "homepage": "https://github.com/playerony/lifestyle-blog#readme",
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "concurrently": "^5.0.2"
  }
}
