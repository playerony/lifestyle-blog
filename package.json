{
  "name": "lifestyle-blog",
  "version": "1.0.0",
  "description": "It's a code of my personal blog about lifestyle. Here is a backend and two app panels configured in the one project.",
  "scripts": {
    "admin:test": "npm run test --prefix admin",
    "server:test": "npm run test --prefix server",
    "test": "concurrently \"npm run server:test\" \"npm run admin:test\"",

    "admin": "npm start --prefix admin",
    "server": "npm start --prefix server",
    "start": "concurrently \"npm run server\" \"npm run admin\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playerony/lifestyle-blog.git"
  },
  "keywords": [
    "lifestyle",
    "blog"
  ],
